{
  "name": "polyenvironments",
  "description": "This file describes the environment and add-ons required to run an app on Heroku",
  "scripts": {
  },
  "env": {
    "buildpack": {
      "required": true
    },
    "MONGOLAB_URI": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": [
    "mongolab"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ]
}
